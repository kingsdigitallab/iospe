             <title xml:lang="en">Bilingual epitaph of M[---] Tyche</title>
         </titleStmt>
         <publicationStmt><authority>King's College London</authority><idno type="IM2015">30.245</idno><idno type="PE">PE3000565</idno><idno type="filename">3.415</idno></publicationStmt>
