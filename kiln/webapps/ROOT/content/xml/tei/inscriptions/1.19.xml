               		<lb n="2"/><space quantity="2" unit="character"/> <w>ων</w> <w>τὸν</w> 
                                          <w>ν<supplied reason="lost" cert="low">αὸν</supplied></w> <gap reason="lost" extent="unknown" unit="character"/>
