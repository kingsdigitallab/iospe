         <div type="edition" xml:lang="grc" xml:space="preserve">
            <ab>
               	<lb n="1"/><persName type="attested" ref="#person972"><name nymRef="Χάρμιππος">Χάρμιππος</name> 
                                   <persName type="attested" ref="#person973"><name nymRef="Πρύτανις">Πρυτάνιος</name></persName></persName>
               	<lb n="2"/><w>κατ’</w> <w>ἐπίταγμα</w>
               	<lb n="3"/><persName type="divine" key="sarapis"><name>Σαράπι</name></persName> <space extent="unknown" unit="character"/>, 
                                 <persName type="divine" key="isis"><name>Ἴσι</name></persName> <space extent="unknown" unit="character"/>, 
                                 <persName type="divine" key="anubis"><name>Ἀνούβι</name></persName>.
