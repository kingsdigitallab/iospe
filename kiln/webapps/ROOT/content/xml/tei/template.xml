<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.stoa.org/epidoc/schema/latest/tei-epidoc.rng" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.stoa.org/epidoc/schema/dev/ircyr-checking.sch" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
    <teiHeader>
        <fileDesc>
            <titleStmt> <!-- \d+\. -->
                <title xml:lang="ru"></title>
                <title xml:lang="en"></title>
            </titleStmt>
            <publicationStmt>
                <idno type="filename"></idno>
            </publicationStmt>
            <sourceDesc>
                <msDesc>
                    <msIdentifier>
                        <altIdentifier xml:lang="ru">
                            <!-- Институт хранения -->
                            <repository></repository>
                            <idno/>
                        </altIdentifier>
                        <altIdentifier xml:lang="en">
                            <repository></repository>
                            <idno/>
                        </altIdentifier>
                    </msIdentifier>
                    <msContents><summary>
                        <!-- Характер документа -->
                        <seg xml:lang="ru"></seg>
                        <seg xml:lang="en"></seg>
                    </summary></msContents>
                    <physDesc>
                        <objectDesc>
                            <supportDesc>
                                <support>
                                    <!-- Разновидность -->
                                    <objectType xml:lang="ru"></objectType>
                                    <objectType xml:lang="en"></objectType>
                                    <!-- Материал -->
                                    <material xml:lang="ru"></material>
                                    <material xml:lang="en"></material>
                                    <!-- Описание и состояние документа -->
                                    <p xml:lang="ru"></p>
                                    <p xml:lang="en"></p>
                                    <!-- Размеры -->
                                    <dimensions>
                                        <height></height><!-- Высота  -->
                                        <width></width><!-- ширина  -->
                                        <depth></depth><!-- толщина -->
                                    </dimensions>
                                </support>
                            </supportDesc>
                            <layoutDesc>
                                <!-- Местоположение -->
                                <layout ana="f1"><!-- ana= Код фаса -->
                                    <seg xml:lang="ru"></seg>
                                    <seg xml:lang="en"></seg>
                                </layout>
                            </layoutDesc>
                        </objectDesc>
                        <handDesc>
                            <!-- Стиль письма -->
                            <handNote>
                                <seg xml:lang="ru"></seg>
                                <seg xml:lang="en"></seg>
                                <height unit="cm"></height><!-- Высота букв -->
                            </handNote>
                        </handDesc>
                    </physDesc>
                <history>
                    <origin>
                        <!-- Происхождение текста -->
                        <origPlace>
                            <seg xml:lang="ru"></seg>
                            <seg xml:lang="en"></seg>
                        </origPlace>
                        
                        <!-- Датировка текста -->
                        <origDate notBefore="0001" notAfter="0001" evidence="0"><!-- evidence= Обоснование датировки -->
                            <seg xml:lang="ru"></seg>
                            <seg xml:lang="en"></seg>
                        </origDate>
                    </origin>
                    
                        
                            <!-- Автопсия -->
                            <provenance type="autopsy">
                                <seg xml:lang="ru"></seg>
                                <seg xml:lang="en"></seg>
                            </provenance>
                            
                            <!-- Место находки -->
                            <provenance type="found">
                                <seg xml:lang="ru">
                                    <placeName type="ancientFindspot"></placeName>: 
                                    <!-- Условия находки -->
                                    <rs type="circumstances"></rs>
                                    <!-- Контекст находки -->
                                    <rs type="context"></rs>
                                </seg>
                                <seg xml:lang="en">
                                    <placeName type="ancientFindspot"></placeName>: 
                                    <rs type="circumstances"></rs>
                                    <rs type="context"></rs>
                                </seg>
                            </provenance>
                            
                            <!-- Место хранения -->
                            <provenance type="observed">
                                <seg xml:lang="ru"></seg>
                                <seg xml:lang="en"></seg>
                            </provenance>
                </history>
            </msDesc>
            </sourceDesc>
        </fileDesc>
    </teiHeader>
    <text>
        <body>
            
            <div type="bibliography" xml:lang="ru">
            <!-- Издания -->
                <listBibl>
                    <bibl n="1"></bibl>
                </listBibl>
            </div>
            <div type="bibliography" xml:lang="en">
                <listBibl>
                    <bibl n="1"></bibl>
                </listBibl>
            </div>
            
        <div type="edition" xml:lang="grc" xml:space="preserve">
            <ab>
                <lb n="1"/>
            </ab>
        </div>
            
            <div type="apparatus">
                <!-- Критич. аппарат -->
                <p></p>
            </div>
            
            <div type="translation" xml:lang="ru">
                <!-- Перевод -->
                <p>
                </p>
            </div>
            <div type="translation" xml:lang="en">
                <p></p>
            </div>
            
            <div type="commentary" xml:lang="ru">
                <!-- Комментарий -->
                <p></p>
            </div>
            <div type="commentary" xml:lang="en">
                <p></p>
            </div>
        </body>
    </text>
</TEI>